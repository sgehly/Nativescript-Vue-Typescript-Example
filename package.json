{
  "name": "nativescript-template-thing",
  "version": "1.0.0",
  "description": "HELP",
  "author": "Sam Gehly <sam@gehly.net>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --env.tnsAction=build",
    "build:android": "webpack --env.tnsAction=build --env.android",
    "build:ios": "webpack --env.tnsAction=build --env.ios",
    "debug": "webpack --watch --env.tnsAction=debug",
    "debug:android": "webpack --env.tnsAction=debug --env.android --watch",
    "debug:ios": "webpack --env.tnsAction=debug --env.ios --watch",
    "watch": "webpack --watch --env.tnsAction=run",
    "watch:android": "webpack --env.tnsAction=run --env.android --watch",
    "watch:ios": "webpack --env.tnsAction=run --env.ios --watch",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "nativescript": "^4.1.0",
    "nativescript-theme-core": "*",
    "nativescript-vue": "^1.4.0-alpha.0",
    "nativescript-vue-target": "git+https://github.com/sgehly/nativescript-vue-target.git",
    "ns-vue-loader": "git+https://github.com/sgehly/ns-vue-loader.git",
    "postcss-loader": "^2.1.5",
    "tns-core-modules": "*",
    "ts-loader": "^4.4.1",
    "typescript": "^2.9.1",
    "vue": "^2.5.16",
    "webpack": "^4.12.0"
  },
  "devDependencies": {
    "babel-core": "*",
    "babel-loader": "*",
    "babel-plugin-transform-object-rest-spread": "*",
    "babel-preset-env": "*",
    "copy-webpack-plugin": "*",
    "css-loader": "*",
    "extract-text-webpack-plugin": "*",
    "fs-extra": "*",
    "mini-css-extract-plugin": "^0.4.0",
    "nativescript-vue-externals": "*",
    "nativescript-vue-loader": "*",
    "nativescript-vue-template-compiler": "*",
    "node-sass": "^4.9.0",
    "optimize-css-assets-webpack-plugin": "*",
    "rimraf": "*",
    "sass-loader": "*",
    "style-loader": "*",
    "vue-template-compiler": "*",
    "webpack-cli": "^3.0.4",
    "webpack-synchronizable-shell-plugin": "*",
    "winston-color": "*"
  }
}
